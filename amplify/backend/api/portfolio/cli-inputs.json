{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "portfolio",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "api key description",
      "expirationTime": 30
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      },
      {
        "mode": "AMAZON_COGNITO_USER_POOLS",
        "cognitoUserPoolId": "authportfolio"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "NONE"
      }
    }
  }
}